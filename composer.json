{
    "name": "ferblancodosil/redirectorphp",
    "description": "A PHP App Test based on PHP Slim",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Blanc"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.3",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }
}
